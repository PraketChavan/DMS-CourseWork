<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity" prefHeight="245.0"
      prefWidth="471.0" xmlns="http://javafx.com/javafx/11.0.2"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.migrateFx.controller.game.PauseMenuController">
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label alignment="CENTER" contentDisplay="CENTER" text="PAUSE MENU">
        <font>
            <Font size="23.0"/>
        </font>
    </Label>
    <Pane prefHeight="200.0" prefWidth="200.0">
        <Button fx:id="m_Exit" layoutX="20.0" layoutY="133.0"
                mnemonicParsing="false" onAction="#onExitClick"
                prefHeight="44.0" prefWidth="108.0" text="Exit">
            <font>
                <Font size="20.0"/>
            </font>
        </Button>
        <Button fx:id="m_Restart" layoutX="20.0" layoutY="70.0"
                mnemonicParsing="false" onAction="#onRestartClick"
                prefHeight="44.0" prefWidth="108.0" text="Restart">
            <font>
                <Font size="20.0"/>
            </font>
        </Button>
        <Button fx:id="m_Continue" alignment="TOP_LEFT" layoutX="20.0"
                layoutY="14.0" mnemonicParsing="false"
                onAction="#onContinueClick" text="Continue">
            <font>
                <Font size="20.0"/>
            </font>
        </Button>
    </Pane>
</VBox>
