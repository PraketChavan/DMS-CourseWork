<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.migrateFx.controller.game.GameEndController">
   <top>
      <Label style="-fx-font-weight: 500;" text="GAME COMPLETED!!" textFill="LIME" BorderPane.alignment="CENTER">
         <font>
            <Font size="35.0" />
         </font>
      </Label>
   </top>
   <padding>
      <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
   </padding>
   <center>
       <VBox alignment="CENTER" BorderPane.alignment="CENTER">
           <Label text="HIGHSCORE">
              <font>
                  <Font size="16.0"/>
              </font>
           </Label>
           <TableView fx:id="m_HighScore" prefHeight="200.0" prefWidth="540.0">
               <columns>
                   <TableColumn fx:id="m_PlayerName"
                                prefWidth="413.20000302791595" text="C1"/>
                   <TableColumn fx:id="m_PlayerScore"
                                prefWidth="125.199951171875" text="C2"/>
               </columns>
           </TableView>
           <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0"
                 spacing="10.0">
               <TextField fx:id="m_Name" promptText="Enter Name"/>
               <Button fx:id="m_Submit" mnemonicParsing="false"
                       onAction="#onSubmitClick" text="Submit"/>
           </HBox>
       </VBox>
   </center>
   <bottom>
       <HBox alignment="CENTER" prefHeight="48.0" prefWidth="540.0"
             spacing="100.0" BorderPane.alignment="CENTER">
           <Button fx:id="m_Restart" mnemonicParsing="false"
                   onAction="#onRestartClick" text="Restart"/>
           <Button fx:id="m_Exit" mnemonicParsing="false"
                   onAction="#onExitClick" prefWidth="53.6" text="Exit"/>
       </HBox>
   </bottom>
</BorderPane>
